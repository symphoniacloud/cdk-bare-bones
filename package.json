{
    "name": "cdk-bare-bones",
    "version": "2022.1.0",
    "homepage": "https://github.com/symphoniacloud/cdk-bare-bones",
    "license": "MIT",
    "author": {
        "email": "mike@symphonia.io",
        "name": "Mike Roberts",
        "url": "https://symphonia.io"
    },
    "devDependencies": {
        "@tsconfig/node16": "1.x",
        "@types/node": "16.x",
        "aws-cdk": "2.x",
        "aws-cdk-lib": "2.x",
        "constructs": "10.x",
        "ts-node": "10.x",
        "typescript": "4.x"
    },
    "scripts": {
        "precdk-command": "echo \"Running CDK with command: $npm_config_command\"",
        "cdk-command": "npx cdk $npm_config_command --output build/cdk.out --require-approval never",
        "deploy": "npm run cdk-command --command=deploy",
        "cdk-diff" : "npm run cdk-command --command=diff",
        "cdk-destroy" : "npm run cdk-command --command=destroy"
    }
}
